guard FieldBase: {
	type: string,
	nullable: boolean
};

guard BigIntField: FieldBase & {
	type: "bigint"
};

guard BinaryField: FieldBase & {
	type: "binary"
};

guard BooleanField: FieldBase & {
	type: "boolean"
};

guard NumberField: FieldBase & {
	type: "number"
};

guard StringField: FieldBase & {
	type: "string",
	search?: boolean
};

guard Field: BigIntField | BinaryField | BooleanField | NumberField | StringField;

guard Store: {
	block: number,
	search: number,
	fields: { Field },
	keys: string[],
	indices: { Index }
};

guard Index: {
	block: number,
	keys: string[]
};

guard Link: {
	parent: string,
	child: string,
	keys: { string }
};

guard Database: {
	stores: { Store },
	links: { Link }
};
