		"compile": "rm -rf ./build/* && npx tsc --build",
		"package": "rm -rf ./dist/* && cp -r ./build/* ./dist/",
		"preversion": "npm run build && git add ./",
		"postversion": "git push && git push --tags"
	},
	"version": "0.7.0"
