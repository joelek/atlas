{
	"bin": {},
	"dependencies": {
		"@joelek/bedrock": "github:joelek/bedrock#semver:^0.5",
		"@joelek/ts-stdlib": "github:joelek/ts-stdlib#semver:^1.1"
	},
	"devDependencies": {
		"@types/node": "^16.7",
		"typescript": "^4.4"
	},
	"license": "MIT",
	"main": "./dist/lib/index.js",
	"name": "@joelek/atlas",
	"repository": {
		"type": "git",
		"url": "https://github.com/joelek/atlas.git"
	},
	"scripts": {
		"build": "npm run codegen && npm run compile && npm run package",
		"clean": "rm -rf ./build/ ./node_modules/",
